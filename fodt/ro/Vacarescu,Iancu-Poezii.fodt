     <table:table-cell table:style-name="Table1.A1" office:value-type="string">
      <text:p text:style-name="Trailer"><text:span text:style-name="T45">Apărută în volumul </text:span><text:span text:style-name="T46">Colecție din poeziile d-lui marelui logofăt I. Văcărescu</text:span><text:span text:style-name="T47">, cap. </text:span><text:span text:style-name="T46">Sfaturi practice</text:span><text:span text:style-name="T47"> (1848)</text:span></text:p>
     </table:table-cell>
