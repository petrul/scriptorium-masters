   <text:p text:style-name="Title">O sam de cuvinte</text:p>
   <text:p text:style-name="Author">Neculce, Ion</text:p>
   <text:p text:style-name="P59"><text:a xlink:type="simple" xlink:href="https://ro.wikisource.org/wiki/Letopiseul_rii_Moldovei/O_sam_de_cuvinte" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="T19">https://ro.wikisource.org/wiki/Letopiseul_rii_Moldovei/O_sam_de_cuvinte</text:span></text:a><text:span text:style-name="T19"><text:line-break/></text:span></text:p>
