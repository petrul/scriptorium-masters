   <text:p text:style-name="P5">FRIEDRICH NIETZSCHE</text:p>
   <text:h text:style-name="Heading_20_1" text:outline-level="1">Ein KÃ¤mpfer Gegen Seine Zeit.</text:h>
   <text:p text:style-name="P5"/>
   <text:p text:style-name="P5"/>
   <text:p text:style-name="P5">FRIEDRICH NIETZSCHE</text:p>
   <text:p text:style-name="P5">EIN KÃMPFER GEGEN SEINE ZEIT.</text:p>
   <text:p text:style-name="P5">Von<text:line-break/>Dr. RUDOLF STEINER.</text:p>
   <text:p text:style-name="P5">WEIMAR.<text:line-break/>VERLAG VON EMIL FELBER.<text:line-break/>1895.</text:p>
   <text:p text:style-name="P5"/>
