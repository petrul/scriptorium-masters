   <text:p text:style-name="Title">Ein Hungerk√ºnstler</text:p>
   <text:p text:style-name="Author">Franz Kafka</text:p>
   <text:p text:style-name="Date">2022-08-14T10:36:15.440361+00:002009-12-12</text:p>
   <text:p text:style-name="Text_20_body"/>
   <text:p text:style-name="Text_20_body"/>
