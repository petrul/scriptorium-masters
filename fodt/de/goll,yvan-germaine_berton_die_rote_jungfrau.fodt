   <text:p text:style-name="Title">Germaine Berton, die rote Jungfrau</text:p>
   <text:p text:style-name="Author">Yvan Goll</text:p>
   <text:h text:style-name="Heading_20_1" text:outline-level="1">Germaine Berton, die rote Jungfrau</text:h>
   <text:p text:style-name="Date">2022-07-29T12:46:37.926090+00:002021-06-19</text:p>
   <text:p text:style-name="Text_20_body"/>
   <text:p text:style-name="Text_20_body"/>
