   <text:p text:style-name="Title">Der Dreispitz</text:p>
   <text:p text:style-name="P3">Alarc√≥n<text:span text:style-name="T2">, </text:span>Pedro Antonio de </text:p>
   <text:p text:style-name="Date">2022-08-22T20:36:11.460588+00:002015-10-14</text:p>
   <text:p text:style-name="Text_20_body"/>
   <text:p text:style-name="Text_20_body"/>
