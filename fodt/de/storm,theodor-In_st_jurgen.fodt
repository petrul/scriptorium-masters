   <text:p text:style-name="Title">In St. JÃ¼rgen: Novelle / (1867)</text:p>
   <text:p text:style-name="Author">Theodor Storm</text:p>
   <text:p text:style-name="Date">2022-08-04T14:10:11.083198+00:002005-09-01</text:p>
   <text:p text:style-name="Text_20_body"/>
