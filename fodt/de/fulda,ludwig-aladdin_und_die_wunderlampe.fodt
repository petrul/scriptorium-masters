   <text:p text:style-name="Title">Aladdin und die Wunderlampe / Tausend und einer Nacht nacherzaehlt</text:p>
   <text:p text:style-name="Author">Ludwig Fulda</text:p>
   <text:p text:style-name="Date">2022-08-07T09:09:54.646735+00:002004-11-30</text:p>
   <text:p text:style-name="lg"/>
   <text:p text:style-name="lg"/>
