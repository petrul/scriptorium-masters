   <text:p text:style-name="Title">Erste Gedichte</text:p>
   <text:p text:style-name="P3">Rilke<text:span text:style-name="T1">, </text:span>Rainer Maria </text:p>
   <text:p text:style-name="Date">2022-08-15T13:49:46.147649+00:002010-09-30</text:p>
   <text:p text:style-name="lg"/>
   <text:p text:style-name="lg"/>
