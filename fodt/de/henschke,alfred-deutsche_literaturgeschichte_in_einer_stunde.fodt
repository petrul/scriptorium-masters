   <text:p text:style-name="Title">Deutsche Literaturgeschichte in einer Stunde / Von den Ã¤ltesten Zeiten bis zur Gegenwart</text:p>
   <text:p text:style-name="Author">Alfred Henschke</text:p>
   <text:p text:style-name="Date">2022-08-10T15:00:59.519672+00:002007-09-05</text:p>
   <text:p text:style-name="Text_20_body"/>
