   <text:p text:style-name="Title">Kater Martinchen</text:p>
   <text:p text:style-name="P3">Arndt<text:span text:style-name="T1">, </text:span>Ernst Moritz </text:p>
   <text:p text:style-name="Date">2022-08-03T14:54:10.094772+00:002004-10-01</text:p>
   <text:p text:style-name="Text_20_body"/>
   <text:p text:style-name="Text_20_body"/>
