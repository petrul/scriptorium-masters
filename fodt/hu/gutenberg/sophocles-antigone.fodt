   <text:p text:style-name="Title">Antigone</text:p>
   <text:p text:style-name="Author">Sophocles</text:p>
   <text:p text:style-name="Date">2022-09-29T07:31:26.561422+00:002021-01-27</text:p>
   <text:p text:style-name="lg"/>
