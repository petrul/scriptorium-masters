   <text:h text:style-name="Heading_20_1" text:outline-level="1">Philoktetes</text:h>
   <text:p text:style-name="First_20_paragraph"></text:p>
   <text:p text:style-name="P7">IRTA</text:p>
   <text:p text:style-name="P7">SOPHOKLES.</text:p>
   <text:p text:style-name="P7"></text:p>
   <text:p text:style-name="P7">GRGBL FORDITOTTA, BEVEZETTE S JEGYZETEKKEL KISRTE</text:p>
   <text:p text:style-name="P7">CSIKY GERGELY.</text:p>
