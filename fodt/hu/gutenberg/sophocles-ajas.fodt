   <text:p text:style-name="Title">Ajas: Szomorujáték</text:p>
   <text:p text:style-name="Author">Sophocles</text:p>
   <text:p text:style-name="Date">2022-08-25T19:24:04.129860+00:002018-04-04</text:p>
   <text:p text:style-name="lg"/>
   <text:p text:style-name="lg"/>
   <text:p text:style-name="lg"/>
   <text:p text:style-name="lg"/>
