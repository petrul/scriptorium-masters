   <text:p text:style-name="Title">Elektra</text:p>
   <text:p text:style-name="Author">Sophocles</text:p>
   <text:p text:style-name="Date">2022-08-25T18:18:13.387482+00:002018-03-15</text:p>
   <text:p text:style-name="lg"/>
   <text:p text:style-name="lg"/>
