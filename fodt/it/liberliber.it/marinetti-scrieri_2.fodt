      <text:p text:style-name="P121">Quarta categoria:</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="TableBox94575101954080" office:value-type="string">
      <text:p text:style-name="P185">quasi irritante, caldo, volitivo.</text:p>
      <text:p text:style-name="P186">Seta granulosa,</text:p>
      <text:p text:style-name="P186">Seta intrecciata,</text:p>
      <text:p text:style-name="P186">Stoffa spugnosa.</text:p>
