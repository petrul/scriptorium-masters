   <text:p text:style-name="P252">AMRI, scudiero di Guntigi.</text:p>
   <text:p text:style-name="P252">SVARTO, soldato.</text:p>
   <text:p text:style-name="P6"><text:span text:style-name="T76">FRANCHI</text:span></text:p>
   <text:p text:style-name="P252">CARLO, re.</text:p>
   <text:p text:style-name="P252">ALBINO, legato.</text:p>
   <table:table table:name="Tabella3" table:style-name="Tabella3">
    <table:table-column table:style-name="Tabella3.A"/>
    <table:table-column table:style-name="Tabella3.B"/>
