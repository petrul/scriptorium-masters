      <text:p text:style-name="P22">}</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Tabella1.B1" office:value-type="string">
      <text:p text:style-name="P6">Claudia Fantuzzi Paltroni</text:p>
     </table:table-cell>
