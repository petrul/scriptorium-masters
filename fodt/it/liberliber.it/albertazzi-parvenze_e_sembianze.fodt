   </table:table>
   <text:p text:style-name="P26"><text:span text:style-name="T2">Il buondí</text:span><text:span text:style-name="T4"> <text:s text:c="3"/>Clementina Orsi Ercolani</text:span></text:p>
   <text:p text:style-name="P26"><text:span text:style-name="T2">Il falcone</text:span><text:span text:style-name="T4"> <text:s text:c="2"/>Orsina Foscherari Favi</text:span></text:p>
   <table:table table:name="Tabella2" table:style-name="Tabella2">
