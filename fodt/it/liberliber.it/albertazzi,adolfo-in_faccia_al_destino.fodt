   <text:p text:style-name="P10"/>
   <text:h text:style-name="Heading_20_1" text:outline-level="1">In faccia al destino</text:h>
   <text:p text:style-name="P2"/>
   <text:p text:style-name="Subtitle">Romanzo</text:p>
   <text:p text:style-name="P2"/>
   <text:p text:style-name="Subtitle">di</text:p>
   <text:p text:style-name="P2"/>
   <text:p text:style-name="Author">Adolfo Albertazzi</text:p>
   <text:p text:style-name="P2"/>
   <text:p text:style-name="P2"/>
   <text:p text:style-name="P2"/>
   <text:p text:style-name="P2"/>
