   <text:p text:style-name="P30">Adolfo Albertazzi</text:p>
   <text:p text:style-name="LL_3a__20_titolo_20_libro">In faccia al destino</text:p>
   <text:p text:style-name="P45"/>
   <text:p text:style-name="P29"/>
