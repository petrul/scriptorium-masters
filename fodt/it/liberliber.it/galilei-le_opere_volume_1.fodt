       <text:p text:style-name="footnote_20_text"><text:span text:style-name="T10">Chrysti. –</text:span> [<text:span text:style-name="T20">Correzione</text:span>]</text:p></text:note-body></text:note></text:span><text:span text:style-name="T92">, qui natus est 194</text:span><text:span text:style-name="T58">a</text:span><text:span text:style-name="T92"> Olympiade</text:span><text:span text:style-name="Footnote_20_anchor"><text:note text:id="ftn87" text:note-class="footnote"><text:note-citation>87</text:note-citation><text:note-body>
       <text:p text:style-name="footnote_20_text"><text:span text:style-name="Footnote_20_Symbol"><text:span text:style-name="T46"><text:s/></text:span></text:span><text:span text:style-name="T10">Olimpiade. –</text:span> [<text:span text:style-name="T20">Correzione</text:span>]</text:p></text:note-body></text:note></text:span><text:span text:style-name="T92">, 560; anni ab ortu Christi usque ad </text:span><text:span text:style-name="T92">excidium Hierosolymorum, 74; illinc usque ad praesens</text:span><text:span text:style-name="Footnote_20_anchor"><text:note text:id="ftn88" text:note-class="footnote"><text:note-citation>88</text:note-citation><text:note-body>
       <text:p text:style-name="footnote_20_text"><text:span text:style-name="Footnote_20_Symbol"><text:span text:style-name="T46"><text:s/></text:span></text:span><text:span text:style-name="T10">presens. –</text:span> [<text:span text:style-name="T20">Correzione</text:span>]</text:p></text:note-body></text:note></text:span><text:span text:style-name="T92"> tempus, 1510.</text:span></text:p>
