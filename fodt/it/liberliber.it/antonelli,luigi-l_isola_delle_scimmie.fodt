      <text:p text:style-name="Personaggio">Argia.</text:p>
      <text:p text:style-name="Text_20_body"><text:soft-page-break/>Basta!</text:p>
     </table:table-cell>
     <table:covered-table-cell table:style-name="Tabella2.A1"/>
