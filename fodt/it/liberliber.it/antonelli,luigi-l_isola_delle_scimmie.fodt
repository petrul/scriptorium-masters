     <table:table-cell table:style-name="Tabella8.A1" office:value-type="string">
      <text:p text:style-name="Text_20_body">Oh!</text:p>
     </table:table-cell>
     <table:covered-table-cell table:style-name="Tabella8.A1"/>
     <table:covered-table-cell table:style-name="Tabella8.C1"/>
    </table:table-row>
