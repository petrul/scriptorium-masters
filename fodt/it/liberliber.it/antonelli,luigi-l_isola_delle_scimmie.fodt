     <table:table-cell table:style-name="Tabella2.A1" office:value-type="string">
      <text:p text:style-name="Personaggio">Malbruk.</text:p>
      <text:p text:style-name="Text_20_body">Lasciate che dica le sue ragioni!</text:p>
     </table:table-cell>
     <table:covered-table-cell table:style-name="Tabella2.A1"/>
     <table:covered-table-cell table:style-name="Tabella2.A1"/>
    </table:table-row>
