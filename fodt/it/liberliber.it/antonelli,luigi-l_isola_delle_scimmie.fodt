    <table:table-row>
     <table:table-cell table:style-name="Tabella6.A1" office:value-type="string">
      <text:p text:style-name="P20">– Oh! mio caro Conte...</text:p>
     </table:table-cell>
     <table:covered-table-cell table:style-name="Tabella6.A1"/>
     <table:covered-table-cell table:style-name="Tabella6.A1"/>
    </table:table-row>
    <table:table-row>
     <table:table-cell table:style-name="Tabella6.A1" office:value-type="string">
      <text:p text:style-name="P20">– Sono molto contento.</text:p>
     </table:table-cell>
     <table:covered-table-cell table:style-name="Tabella6.A1"/>
     <table:covered-table-cell table:style-name="Tabella6.A1"/>
    </table:table-row>
    <table:table-row>
     <table:table-cell table:style-name="Tabella6.A1" office:value-type="string">
      <text:p text:style-name="P20">– Grazie, e lei?</text:p>
     </table:table-cell>
     <table:covered-table-cell table:style-name="Tabella6.A1"/>
     <table:covered-table-cell table:style-name="Tabella6.A1"/>
    </table:table-row>
    <table:table-row>
     <table:table-cell table:style-name="Tabella6.A1" office:value-type="string">
      <text:p text:style-name="P20">– Caro Avvocato. Oh! Baronessa!</text:p>
     </table:table-cell>
     <table:covered-table-cell table:style-name="Tabella6.A1"/>
     <table:covered-table-cell table:style-name="Tabella6.A1"/>
    </table:table-row>
