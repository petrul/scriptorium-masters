      <text:p text:style-name="Personaggio">Argia.</text:p>
      <text:p text:style-name="Text_20_body">Perchè?</text:p>
     </table:table-cell>
     <table:covered-table-cell table:style-name="Tabella1.A1"/>
     <table:covered-table-cell table:style-name="Tabella1.A1"/>
    </table:table-row>
    <table:table-row>
     <table:table-cell table:style-name="Tabella1.A1" office:value-type="string">
      <text:p text:style-name="Personaggio">Malbruk.</text:p>
      <text:p text:style-name="Text_20_body">Ha ragione.</text:p>
     </table:table-cell>
     <table:covered-table-cell table:style-name="Tabella1.A1"/>
     <table:covered-table-cell table:style-name="Tabella1.A1"/>
    </table:table-row>
