      <text:p text:style-name="Personaggio">Patas.</text:p>
      <text:p text:style-name="Text_20_body">Non è vero!</text:p>
     </table:table-cell>
     <table:covered-table-cell table:style-name="Tabella1.A1"/>
     <table:covered-table-cell table:style-name="Tabella1.A1"/>
