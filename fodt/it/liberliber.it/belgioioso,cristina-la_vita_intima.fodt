   <text:p text:style-name="Text_20_body"><text:span text:style-name="small1"><text:span text:style-name="T13">ALLA CARA MEMORIA</text:span></text:span></text:p>
   <text:p text:style-name="Text_20_body"><text:span text:style-name="x-small1"><text:span text:style-name="T15">DELLA</text:span></text:span></text:p>
   <text:p text:style-name="Text_20_body"><text:span text:style-name="smcap1"><text:span text:style-name="T14">Marchesa</text:span></text:span><text:span text:style-name="T14"> MARIA TROTTI-BENTIVOGLIO</text:span></text:p>
   <text:p text:style-name="Text_20_body"><text:span text:style-name="smcap1"><text:span text:style-name="T14">BARBIANO di BELGIOJOSO</text:span></text:span></text:p>
   <text:p text:style-name="Text_20_body"><text:span text:style-name="small1"><text:span text:style-name="T13">COMPAGNA ALLA MADRE IN QUESTO VIAGGIO D&apos;ORIENTE</text:span></text:span></text:p>
   <text:p text:style-name="Text_20_body"><text:span text:style-name="small1"><text:span text:style-name="T13">TESTIMONE E PARTECIPE</text:span></text:span></text:p>
   <text:p text:style-name="Text_20_body"><text:span text:style-name="small1"><text:span text:style-name="T13">DEI SUOI PATRIOTTICI ARDIMENTI</text:span></text:span></text:p>
   <text:p text:style-name="P68"><text:span text:style-name="small1"><text:span text:style-name="T13"/></text:span></text:p>
   <text:p text:style-name="Text_20_body"><text:span text:style-name="small1"><text:span text:style-name="T13"/></text:span></text:p>
