    <text:list-item>
     <text:p text:style-name="P41"><text:a xlink:type="simple" xlink:href="#c5_Qui_a_voulu_la_guerre___4.xhtml#cite_ref-5-n264" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="Definition"></text:span></text:a> <text:span text:style-name="T1">Cor. B.</text:span>, n<text:span text:style-name="T5">o</text:span>121.</text:p>
