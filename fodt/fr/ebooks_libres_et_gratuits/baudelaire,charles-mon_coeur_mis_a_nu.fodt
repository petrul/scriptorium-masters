   <text:p text:style-name="Text_20_body">A ajouter aux mÃ©taphores militaires :</text:p>
   <text:p text:style-name="Text_20_body"/>
   <text:p text:style-name="Text_20_body">Les poÃ¨tes de combat.</text:p>
