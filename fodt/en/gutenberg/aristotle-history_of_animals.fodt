     <text:p text:style-name="P12">sleep, <text:a xlink:type="simple" xlink:href="#346190705085067958_59058-h-6.htm.xhtml#Pg098" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="Definition">98</text:span></text:a>;</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P12">kinds, <text:a xlink:type="simple" xlink:href="#346190705085067958_59058-h-6.htm.xhtml#Pg099" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="Definition">99</text:span></text:a>, <text:a xlink:type="simple" xlink:href="#346190705085067958_59058-h-7.htm.xhtml#Pg104" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="Definition">104</text:span></text:a>;</text:p>
    </text:list-item>
    <text:list-item>
