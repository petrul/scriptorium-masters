    <text:list-item>
     <text:p text:style-name="P12">where found, <text:a xlink:type="simple" xlink:href="#346190705085067958_59058-h-7.htm.xhtml#Pg109" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="Definition">109</text:span></text:a>;</text:p>
    </text:list-item>
