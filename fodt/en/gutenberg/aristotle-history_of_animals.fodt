    <text:list-item>
     <text:p text:style-name="P12">, gull or cormorant, Larus canus and marinus, Sterna, <text:a xlink:type="simple" xlink:href="#346190705085067958_59058-h-4.htm.xhtml#Pg045" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="Definition">45</text:span></text:a>;</text:p>
