   <text:p text:style-name="Title">The Red One</text:p>
   <text:p text:style-name="Author">Jack London</text:p>
   <text:p text:style-name="Date">1997-01-01</text:p>
   <text:p text:style-name="Text_20_body"/>
   <text:p text:style-name="Text_20_body"/>
