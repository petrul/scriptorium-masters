      <text:p text:style-name="Table_20_Contents"><text:a xlink:type="simple" xlink:href="#tag40_5" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="Definition">5.</text:span></text:a> <text:span text:style-name="T2">Straffen</text:span>, ‘refute.’</text:p>
      <text:p text:style-name="Table_20_Contents"><text:a xlink:type="simple" xlink:href="#tag40_6" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="Definition">6.</text:span></text:a> <text:span text:style-name="T2">Da kegen</text:span> = <text:span text:style-name="T2">dagegen</text:span>.</text:p>
