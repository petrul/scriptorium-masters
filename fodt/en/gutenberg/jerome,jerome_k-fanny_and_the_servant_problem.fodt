   <text:h text:style-name="Heading_20_1" text:outline-level="1"><text:span text:style-name="T3">Fanny and</text:span><text:line-break/><text:span text:style-name="T3">the Servant Problem</text:span></text:h>
   <text:p text:style-name="P4">A Quite Possible Play in Four Acts</text:p>
   <text:p text:style-name="Text_20_body"><text:span text:style-name="T3">By</text:span><text:line-break/><text:span text:style-name="T4">Jerome K. Jerome</text:span></text:p>
   <text:p text:style-name="Text_20_body"></text:p>
   <text:p text:style-name="Text_20_body"></text:p>
   <text:p text:style-name="Text_20_body"><text:span text:style-name="T3">COPYRIGHT</text:span> 1909 <text:span text:style-name="T3">BY</text:span><text:line-break/><text:span text:style-name="T3">JEROME KLAPKA JEROME</text:span></text:p>
   <text:p text:style-name="Text_20_body"></text:p>
   <text:p text:style-name="Text_20_body"></text:p>
   <text:p text:style-name="Text_20_body"><text:span text:style-name="T4">Hodder and Stoughton</text:span><text:line-break/><text:span text:style-name="T4">Limited</text:span> <text:span text:style-name="T4">London</text:span></text:p>
   <text:p text:style-name="Text_20_body"></text:p>
   <text:p text:style-name="Text_20_body">Amateurs wishing to perform this play should apply to:</text:p>
   <text:p text:style-name="Text_20_body">Samuel French, Ltd.</text:p>
   <text:p text:style-name="Text_20_body">26 Southampton Street,<text:line-break/><text:soft-page-break/>Strand, W.C.2.</text:p>
