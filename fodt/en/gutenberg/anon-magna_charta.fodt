   <text:p text:style-name="Author">Anonymous</text:p>
   <text:p text:style-name="Date">2022-10-05T10:43:40.604585+00:002006-03-01</text:p>
   <text:p text:style-name="Text_20_body"/>
   <text:h text:style-name="Heading_20_1" text:outline-level="1">The Magna Carta</text:h>
   <text:p text:style-name="Horizontal_20_Line"/>
