     <table:table-cell table:style-name="Table1.A1" office:value-type="string">
      <text:p text:style-name="First_20_paragraph"><text:line-break/></text:p>
      <text:p text:style-name="Text_20_body">LISEO, caballero.<text:line-break/>TURÍN, lacayo.<text:line-break/>LEANDRO, caballero.<text:line-break/>OTAVIO, viejo.<text:line-break/></text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table1.A1" office:value-type="string">
      <text:p text:style-name="Text_20_body"><text:line-break/>MISENO, su amigo.<text:line-break/>LAURENCIO, caballero.<text:line-break/>DUARDO, caballero.<text:line-break/>FENISO, caballero.<text:line-break/></text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table1.A1" office:value-type="string">
      <text:p text:style-name="First_20_paragraph">RUFINO, maestro.<text:line-break/>NISE, dama.<text:line-break/>FINEA, su hermana.<text:line-break/>CLARA, criada.<text:line-break/></text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table1.A1" office:value-type="string">
      <text:p text:style-name="Text_20_body"><text:line-break/>CELIA, criada.<text:line-break/>PEDRO, lacayo.<text:line-break/>[MÚSICOS].<text:line-break/>[UN MAESTRO de danzar].<text:line-break/></text:p>
     </table:table-cell>
