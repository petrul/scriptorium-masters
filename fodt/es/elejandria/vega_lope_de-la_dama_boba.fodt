   <text:p text:style-name="Title">La dama boba</text:p>
   <text:p text:style-name="P3">Vega<text:span text:style-name="T2">, </text:span>Lope de </text:p>
   <text:p text:style-name="Date">2020-04-22T22:00:00+00:00</text:p>
   <text:p text:style-name="Text_20_body"/>
   <text:p text:style-name="Text_20_body"/>
