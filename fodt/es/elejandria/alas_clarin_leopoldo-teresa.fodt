   <text:p text:style-name="Title">Teresa</text:p>
   <text:p text:style-name="P3">Alas Clarín<text:span text:style-name="T2">, </text:span>Leopoldo </text:p>
   <text:p text:style-name="Date">1894-12-31T23:00:00+00:00</text:p>
   <text:p text:style-name="Text_20_body"/>
   <text:p text:style-name="Text_20_body"/>
   <text:p text:style-name="Text_20_body"/>
