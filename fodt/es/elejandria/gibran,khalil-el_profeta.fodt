   <text:p text:style-name="Title">El profeta</text:p>
   <text:p text:style-name="Author">Khalil Gibran</text:p>
   <text:p text:style-name="Date">2022-05-25</text:p>
   <text:p text:style-name="Text_20_body"/>
   <text:p text:style-name="Text_20_body"/>
