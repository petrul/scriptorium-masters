   <text:p text:style-name="Title">El corazon delator</text:p>
   <text:p text:style-name="P3">Poe<text:span text:style-name="T3">, </text:span>Edgar Allan </text:p>
   <text:p text:style-name="Date">18712022-03-30</text:p>
   <text:p text:style-name="Text_20_body"/>
   <text:p text:style-name="Text_20_body"/>
   <text:p text:style-name="Text_20_body"/>
